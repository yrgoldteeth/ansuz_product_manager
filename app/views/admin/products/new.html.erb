<%= title "Add a New Product" %>
<% form_for :product, :categories, :html => { :multipart => true }, :url => admin_products_path do |f| %>
  <%= render :partial =>'/admin/products/form', :locals => {:f => f} -%>
  <%= submit_tag("Create Product") -%> or <%= link_to 'Cancel', admin_products_path %>
<% end %>
